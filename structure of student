#include <stdio.h>
struct student
{
    int rno;
    char name[20];
    int marks;
}s;
void main()
{
    printf("\n enter the roll.no of s: ");
    scanf("%d",&s.rno);
    printf("\n enter the name of s: ");
    scanf("%s",s.name);
    printf("\n enter the marks of s: ");
    scanf("%d",&s.marks);
    printf("\n student details: \n");
    printf("roll number:%d",s.rno);
    printf("name:%s",s.name);
    printf("marks:%d",s.marks);
}
